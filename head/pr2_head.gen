component pr2_head{
	version		"1.0";
	lang		"c";
	require		"genom3 >= 2.99.24";
    
    task move{
        period      50ms;
        priority    200;
        stack       4000;

        codel <start>   InitModule() yield ether;
    };

    activity move_head(in double pan, in double tilt){
        codel <start>   mraMove(in pan, in tilt) yield ether;
        task move;
    };
};
